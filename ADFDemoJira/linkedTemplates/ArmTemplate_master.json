{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADFDemoJira"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureBlobStorageapi_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageapi'"
		},
		"Jirademo_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Jirademo'"
		},
		"LSOnPremiseSqlServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LSOnPremiseSqlServer'"
		},
		"AzureKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://KV-DEV-RV.vault.azure.net/"
		},
		"AzureSqlDatabaseDemo_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "NewSQLSEcret"
		},
		"Jirademo_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "https://rayminds.atlassian.net/"
		},
		"Jirademo_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "suman.sen@rayminds.com"
		},
		"LSRestAPI_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://gorest.co.in/public/v2/users"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADFDemoJira_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureBlobStorageapi_connectionString": {
						"value": "[parameters('AzureBlobStorageapi_connectionString')]"
					},
					"Jirademo_password": {
						"value": "[parameters('Jirademo_password')]"
					},
					"LSOnPremiseSqlServer_connectionString": {
						"value": "[parameters('LSOnPremiseSqlServer_connectionString')]"
					},
					"AzureKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault_properties_typeProperties_baseUrl')]"
					},
					"Jirademo_properties_typeProperties_host": {
						"value": "[parameters('Jirademo_properties_typeProperties_host')]"
					},
					"Jirademo_properties_typeProperties_username": {
						"value": "[parameters('Jirademo_properties_typeProperties_username')]"
					},
					"LSRestAPI_properties_typeProperties_url": {
						"value": "[parameters('LSRestAPI_properties_typeProperties_url')]"
					},
					"AzureSqlDatabaseDemo_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('AzureSqlDatabaseDemo_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}